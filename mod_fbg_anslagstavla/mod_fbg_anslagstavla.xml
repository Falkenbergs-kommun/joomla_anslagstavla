	<author>Tomas Bolling Nilsson</author>
	<authorEmail>tomas.bollingnilsson@falkenberg.se</authorEmail>
	<authorUrl>falkenberg.se</authorUrl>
	<copyright>2022</copyright>
	<license>NA</license>
	<version>1.0</version>
	<description>Skapa anslag f√∂r servicewebben</description>

	<files>
		<filename module="mod_fbg_anslagstavla">mod_fbg_anslagstavla.php</filename>
		<folder>tmpl</folder>
		<filename>mod_fbg_anslagstavla.xml</filename>
		<filename>anslagstavla.js</filename>
	</files>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="domain"
					type="url"
					label="MOD_FBG_GROUPS_URL_LABEL"
					description="MOD_FBG_GROUPS_URL_DESCRIPTION"
					hint="https://www.example.com"
					filter="url"
					validate="url"
				/>
			</fieldset>
			<fieldset
					name="advanced">
				<field
						name="moduleclass_sfx"
						type="text"
						label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
						description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
			</fieldset>
		</fields>
	</config>
</extension>
